<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7" />
    </startup>
  <system.serviceModel>
    <services>
      <service name="WcfTest.Service.MyService">
        <endpoint address="net.pipe://localhost/MyService" binding="netNamedPipeBinding"
          contract="WcfTest.Contracts.Service.IMyService" />
      </service>
      <service name="WcfTest.Service.EventRegistrar">
        <endpoint address="net.pipe://localhost/EventRegistrar" binding="netNamedPipeBinding"
          contract="WcfTest.Contracts.Service.IEventRegistrar" />
      </service>
    </services>
  </system.serviceModel>
</configuration>